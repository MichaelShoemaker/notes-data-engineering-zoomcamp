SELECT count(*) 
FROM `dazzling-pier-338723.trips_data_all.fhv_tripdata_partitioned` 
WHERE  dropoff_datetime BETWEEN '2019-01-01' AND '2019-03-31'
and dispatching_base_num in ('B00987', 'B02060', 'B02279')


SELECT count(*) 
FROM `dazzling-pier-338723.trips_data_all.external_fhv_tripdata` 
WHERE  dropoff_datetime BETWEEN '2019-01-01' AND '2019-03-31'
and dispatching_base_num in ('B00987', 'B02060', 'B02279')
